#include<stdlib.h>
#include "main.h"

/**
  *_puts_recursion - prints a string
  *
  *@s: string to print
  *Return: Always 0
  */

void _puts_recursion(char *s)
{
        int i;
        
        for (i = 0; j > 0; j++)
         {
            if (s[i] != '\0')
            put(s);
         }
         return (0);
}
